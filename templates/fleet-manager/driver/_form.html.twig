<div class="row">
    {{ form_start(form, {'attr': {'id': 'new'}}) }}

    <div class="col s12 m6">
        {% include 'fleet-manager/layout/inputs/text.html.twig' with { 'field' : form.name } %}
    </div>

    <div class="col s12 m6">
        {% include 'fleet-manager/layout/inputs/text.html.twig' with { 'field' : form.surname } %}
    </div>

    <div class="col s12 m6">
        {% include 'fleet-manager/layout/inputs/text.html.twig' with { 'field' : form.email } %}
    </div>

    <div class="col s12 m6">
        {% include 'fleet-manager/layout/inputs/text.html.twig' with { 'field' : form.phone } %}
    </div>

    {{ form_end(form) }}
</div>